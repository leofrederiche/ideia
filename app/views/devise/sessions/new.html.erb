<aside id="windows_form">
<h2>Já tem uma conta? Faça Login</h2>
<h5><%= render "devise/shared/links" %></h5>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div>
  	<%= f.email_field :email, autofocus: true, placeholder: "E-mail", id: "input_text_form" %>
  </div>

  <div>
    <%= f.password_field :password, autocomplete: "off", placeholder: "Senha", id: "input_text_form" %>
  </div>

  <div><%= f.submit "Login", id: "btnForm" %></div>


  <% if devise_mapping.rememberable? -%>
    <div><%= f.check_box :remember_me %> <%= f.label :remember_me, "Lembrar" %></div>
  <% end -%>
<% end %>

</aside>